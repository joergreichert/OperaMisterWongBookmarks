<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>
<template 
	   id="opera.template.010"
       autoinsert="true" 
       context="de.abg.jreichert.OperaDSL.Element" 
       deleted="false" 
       description="Adds an folder" 
       enabled="true" 
       name="Folder Template">#FOLDER
	ID=${id}
	NAME=${name}
	CREATED=${created}
	UNIQUEID=${unique_id}
</template>
<template 
	   id="opera.template.020"
       autoinsert="true" 
       context="de.abg.jreichert.OperaDSL.Element" 
       deleted="false" 
       description="Adds an link" 
       enabled="true" 
       name="Link Template">#URL
	ID=${id}
	NAME=${name}
	URL=${url}
	CREATED=${created}
	UNIQUEID=${unique_id}
</template>
</templates>