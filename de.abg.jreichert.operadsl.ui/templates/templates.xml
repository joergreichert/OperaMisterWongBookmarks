<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>
<template 
	name="Folder"
	id="opera.template.010"
	autoinsert="true" 
	context="de.abg.jreichert.OperaDSL.kw_folder" 
	deleted="false" 
	description="Adds an folder" 
	enabled="true" 
	>#FOLDER
	ID=${id}
	NAME=${name}
	CREATED=${created}
	UNIQUEID=${unique_id}
</template>
<template 
	name="Link"
	id="opera.template.020"
	autoinsert="true" 
	context="de.abg.jreichert.OperaDSL.kw_url" 
	deleted="false" 
	description="Adds an link" 
	enabled="true" 
	>#URL
	ID=${id}
	NAME=${name}
	URL=${url}
	CREATED=${created}
	UNIQUEID=${unique_id}
</template>
</templates>